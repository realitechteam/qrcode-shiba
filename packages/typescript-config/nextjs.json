{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
        "lib": [
            "ES2022",
            "DOM",
            "DOM.Iterable"
        ],
        "target": "ES2022",
        "module": "ESNext",
        "jsx": "preserve",
        "plugins": [
            {
                "name": "next"
            }
        ],
        "allowJs": true,
        "noEmit": true
    }
}